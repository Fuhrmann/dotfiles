configuration {
  modi: "combi,drun,window,run";
  combi-modi: "drun,run";
  show-icons: true;
  font: "JetBrains Mono 10";
  ssh-client: "ssh";
  eh: 1;
  combi-hide-mode-prefix: true;
  display-combi: "";
  lines: 10;
  terminal: "alacritty";
  run-shell-command: "{terminal} -e {cmd}";
  width: 25;
}

@theme "~/.cache/wal/colors-rofi-dark"

#window {
  padding: 5px;
}

#listview {
  padding: 0;
}

#element {
  padding: 5px;
}
